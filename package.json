{
  "name": "api-solid",
  "scripts": {
    "dev": "node -r @swc-node/register --watch src/server.ts",
    "start": "node build/server.js",
    "lint": "eslint . --cache --cache-location ./.temp/.eslintcache --max-warnings 0 --fix",
    "build": "pnpm type-check && swc src -d build --strip-leading-paths",
    "clean": "rimraf build",
    "type-check": "tsc",
    "db:studio": "prisma studio",
    "db:migrate-dev": "prisma migrate dev",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
